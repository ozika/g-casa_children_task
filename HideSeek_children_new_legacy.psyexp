    <Routine name="checkPerf">
      <CodeComponent name="code_4" plugin="None">
        <Param val="" valType="extendedCode" updates="constant" name="Before Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="Before JS Experiment"/>
        <Param val="#pracFile   = 'conditions/schedules/%s_first/%s/%s/inputTrain_1.1.xlsx' % (expInfo['train_first'], expInfo['direction of x'], expInfo['starting corner'])&amp;#10;&amp;#10;# variable to check performance threshold&amp;#10;saveACC   = 0 &amp;#10;fileidx   = 1&amp;#10;trainidx = 1&amp;#10;addNrep   = 1&amp;#10;nextPrac  = 99&amp;#10;earlierperf = 0&amp;#10;&amp;#10;#  performance threshold: 70% -&gt; trial number = 25&amp;#10;#  25 (praclen) * 0.7 = 17.5, round down = 17 -&gt; need to have 17 correct to move forward&amp;#10;percthres = 0.7&amp;#10;perfthres = 17&amp;#10;# as a reminder, this is the variable to compare the current accuracy to the performance threshold: compareACC" valType="extendedCode" updates="constant" name="Begin Experiment"/>
